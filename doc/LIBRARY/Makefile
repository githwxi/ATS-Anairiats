prelude:: prelude_top prelude_SATS prelude_DATS prelude_CATS

prelude_top:: ; scp $(ATSHOME)/prelude/*.?ats \
  ats-hwxi@web.sourceforge.net:/home/groups/a/at/ats-lang/htdocs/IMPLEMENTATION/Anairiats/ATS/prelude


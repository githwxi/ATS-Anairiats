/*
** computing the factorial numbers
*/

let
//
  val fact = fix f (n:int): int => if n >= 1 then n * f (n-1) else 1
//
  val fact10 = fact (10)
  val _ = print "fact (10) = "
  val _ = print_int (fact10)
  val _ = print "\n"
in
  ()
end // end of [let]

